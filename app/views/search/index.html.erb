<ul>
  <% @stations.each do |station| %>
    <li><%= station.name %>
      <ul>
        <li>Address: <%= station.address %></li>
        <li>Fuel Types: <%= station.fuel_types %></li>
        <li>Distance: <%= station.distance.round(2) %></li>
        <li>Access: <%= station.access_times %></li>
      </ul>
    </li>
    <br />
  <% end %>
</ul>
